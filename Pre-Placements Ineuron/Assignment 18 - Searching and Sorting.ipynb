    "def lengthOfLIS(nums):\n",
    "    if not nums:\n",
    "        return 0\n",
    "\n",
    "    n = len(nums)\n",
    "    dp = [1] * n\n",
    "\n",
    "    for i in range(1, n):\n",
    "        for j in range(i):\n",
    "            if nums[i] > nums[j]:\n",
    "                dp[i] = max(dp[i], dp[j] + 1)\n",
    "\n",
    "    return max(dp)\n",
    "print(lengthOfLIS([10,9,2,5,3,7,101,18]))\n",
    "print(lengthOfLIS([0,1,0,3,2,3]))\n",
    "print(lengthOfLIS([7,7,7,7,7,7,7]))"
